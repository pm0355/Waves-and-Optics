Cell[CellGroupData[{

Cell["Tutorial 1: Real sinusoids with linear phase evolution", "Section",
 CellChangeTimes->{{3.44060403199936*^9, 3.440604040108579*^9}, {
  3.440950187474407*^9, 3.4409501985057983`*^9}}],

Cell["\<\
The phase of a sinusoid is often not the physical variable of interest.  For \
example, in simple harmonics oscillating systems the phase \[Phi] is a linear \
function of time.  The standard form for this is:\
\>", "Text",
 CellChangeTimes->{{3.440604043858507*^9, 3.4406041374504604`*^9}, {
  3.4406041791684093`*^9, 3.4406041886838517`*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{GridBox[{
        {
         RowBox[{"\[Phi]", "=", 
          RowBox[{
           RowBox[{"\[Omega]", " ", "t"}], "+"}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{"Center"}}}], 
      SubscriptBox["\[Phi]", "0"]}]}
   },
   GridBoxItemSize->{"Columns" -> {{
        Scaled[0.96]}}}], TraditionalForm]]]], "Text",
 CellChangeTimes->{{3.4406041502002153`*^9, 3.4406041739653845`*^9}, {
  3.440604407085908*^9, 3.440604409148369*^9}, {3.440604461319242*^9, 
  3.440604463069208*^9}, {3.4406120615470467`*^9, 3.4406120643595285`*^9}},
 FontSize->16],

Cell[TextData[{
 "The variable \[Omega] is the angular frequency and the variable ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Phi]", "0"], TraditionalForm]]],
 " is the phase constant.  To better understand this relationship between \
phase and time, play with the following simulation.  If you click on the \
little plus signs next to each parameter slider, the value taken by that \
parameter are shown."
}], "Text",
 CellChangeTimes->{{3.4406042193238883`*^9, 3.440604279869601*^9}, {
  3.4406043197438354`*^9, 3.4406043250406084`*^9}, {3.4406052573911915`*^9, 
  3.4406052915092983`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Omega]", " ", "t"}], " ", "+", " ", "\[Phi]0"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "10"}], "}"}], ",", 
     RowBox[{"PlotRange", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "20"}], "}"}]}]}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Omega]", ",", "1", ",", "5"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Phi]0", ",", "0", ",", 
     RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.4406043275249357`*^9, 3.4406043293842754`*^9}, {
  3.440604384648839*^9, 3.440604420851269*^9}, {3.4406044516006784`*^9, 
  3.440604527364849*^9}, {3.4406045725514812`*^9, 3.4406045749420605`*^9}, {
  3.440605011824297*^9, 3.4406050149804864`*^9}},
 Background->RGBColor[1, 0.85, 0.85]],

